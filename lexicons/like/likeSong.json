{
  "lexicon": 1,
  "id": "app.rocksky.like.likeSong",
  "defs": {
    "main": {
      "type": "procedure",
      "description": "Like a song",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "properties": {
            "uri": {
              "type": "string",
              "description": "The unique identifier of the song to like",
              "format": "at-uri"
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "ref",
          "ref": "app.rocksky.song.defs#songViewDetailed"
        }
      }
    }
  }
}
